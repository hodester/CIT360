<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="MissionaryCal.Event" table="eventcalendar">
	<id name="eventId" column="eventId" type="long">
         <generator class="native"/>
    </id>
	<property name="date" column="eventDate" type="date"/>
	<property name="name" column="name" type="string"/>
	<property name="email" column="email"  type="string"/>
	<property name="phone" column="phone" type="string"/>
	<many-to-one name="LkAddress" class="MissionaryCal.Event" column="lkaddress" cascade="all" not-null="false" />
	<property name="notes" column="notes" type="string"/>

  </class>
</hibernate-mapping>